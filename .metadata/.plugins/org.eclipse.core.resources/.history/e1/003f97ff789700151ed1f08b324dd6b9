package Graphics;

import java.awt.Dimension;

public class Render {
	@SuppressWarnings("unused")
	private float x, y;
	@SuppressWarnings("unused")
	private Dimension screenSize;
	private int[] Pixels;
	@SuppressWarnings("unused")
	private int ObjectWidth, ObjectHeight;

	public Render() {
	}

	@SuppressWarnings("unused")
	private void render(float xoffset, float yoffset, int OW, int OH, Dimension ss) {
		for (int y = 0; y < OH; y++) {
			int ya = (int) (y + yoffset);
			if (ya > ss.getHeight() || y < 0)
				break;
			for (int x = 0; x < OW; x++) {
				int xa = (int) (x + xoffset);
				if (xa < 0 || xa > ss.getWidth())
					break;
			}
		}
	}
}
