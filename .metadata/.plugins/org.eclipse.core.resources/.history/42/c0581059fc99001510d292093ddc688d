package Graphics;

public class Render {
	private int[] Pixels;
	private final int Width, Height;

	public Render(int w, int h) {
		Width = w;
		Height = h;
	}

	public int[] getPixels() {
		return Pixels;
	}

	public void setPixels(int[] pixels) {
		Pixels = pixels;
	}

	public int getWidth() {
		return Width;
	}

	public int getHeight() {
		return Height;
	}
}