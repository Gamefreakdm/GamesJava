import javax.swing.JFrame;

public class Main {
	private boolean Running = false;
	private final JFrame MFrame = new JFrame("Loading...");
	private final int Width = 800, Height = 600;

	public static void main() {
		Main m = new Main();
		m.MFrame.setResizable(false);
		m.MFrame.setSize(m.Width, m.Height);
		m.MFrame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		m.MFrame.setTitle("Fransformation");
		m.MFrame.setVisible(true);
		m.Start();
	}

	private void Start() {
		if (!Running)
			Running = true;
		Stop();
	}

	private void Stop() {
		if (!Running)
			System.exit(0);
	}

	private void Run() {
	}

	private void Update() {
	}

	private void Render() {
	}
}