package Graphics;

public class Render {
	private int[] Pixels;
	private int Width, Height;

	public Render(int Width, int Height) {
	}

	public int[] getPixels() {
		return Pixels;
	}

	public void setPixels(int[] pixels) {
		Pixels = pixels;
	}

	public int getWidth() {
		return Width;
	}

	public void setWidth(int width) {
		Width = width;
	}

	public int getHeight() {
		return Height;
	}

	public void setHeight(int height) {
		Height = height;
	}
}