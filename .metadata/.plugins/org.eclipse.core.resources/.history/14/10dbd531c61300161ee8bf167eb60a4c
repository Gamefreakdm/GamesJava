package Input;

public class Controller {
	private double x, z, rot, xa, za, rota;

	public void Update(boolean fw, boolean bw, boolean l, boolean r, boolean tl, boolean tr) {
		double RotSpeed = 1;
		double WalkSpeed = 1;
		double xMove = 0;
		double zMove = 0;
		if (fw)
			zMove++;
		if (bw)
			zMove--;
		if (r)
			xMove++;
		if (l)
			xMove--;
	}
}