package Graphics;

public class Render {
	private final int[] Pixels;
	private final int Width, Height;
	protected final Screen screen;

	public Render(int w, int h, Screen s) {
		Width = w;
		Height = h;
		screen = s;
		Pixels = new int[w * h];
	}

	public int getWidth() {
		return Width;
	}

	public int getHeight() {
		return Height;
	}
}