package Level;

import Graphics.Render.Screen;

public class Level {
	private final int SIZE;
	private final char TYPE;
	private final int[] Tiles;

	public Level(int s, char type) {
		SIZE = s;
		TYPE = type;
		Tiles = new int[s * s];
	}

	public void Render(Screen screen) {
	}

	public void Update() {
	}

	public void GenerateLevel() {
	}

	public int getSIZE() {
		return SIZE;
	}

	public char getTYPE() {
		return TYPE;
	}
}