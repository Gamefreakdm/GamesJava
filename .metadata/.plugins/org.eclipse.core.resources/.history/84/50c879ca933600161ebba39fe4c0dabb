package Level.Tile;

import Graphics.Image.Sprite;

public class Tile {
	private float X, Y;
	private Sprite sprite;
	private boolean IsSolid;

	public Tile() {
	}

	public float getX() {
		return X;
	}

	public void setX(float x) {
		X = x;
	}

	public float getY() {
		return Y;
	}

	public void setY(float y) {
		Y = y;
	}

	public Sprite getSprite() {
		return sprite;
	}

	public void setSprite(Sprite sprite) {
		this.sprite = sprite;
	}
}