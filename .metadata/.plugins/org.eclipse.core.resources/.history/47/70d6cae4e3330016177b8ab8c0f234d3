package GameObject.Mob.Entity;

public abstract class Entity {
	private int ID;
	private float X, Y;
	private int Width, Height;

	public abstract void Update();

	public int getID() {
		return ID;
	}

	public float getX() {
		return X;
	}

	public void setX(float x) {
		X = x;
	}

	public float getY() {
		return Y;
	}

	public void setY(float y) {
		Y = y;
	}

	public int getWidth() {
		return Width;
	}

	public int getHeight() {
		return Height;
	}
}